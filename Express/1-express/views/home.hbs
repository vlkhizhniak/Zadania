<div class="container">
    <h1>Users Information</h1>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Name</th>
                <th>Username</th>
                <th>Street</th>
                <th>Suite</th>
                <th>City</th>
                <th>Zipcode</th>
                <th>Website</th>
            </tr>
        </thead>
        <tbody>
            {{#each user}}
            <tr>
                <td>{{name}}</td>
                <td>{{username}}</td>
                <td>{{address.street}}</td>
                <td>{{address.suite}}</td>
                <td>{{address.city}}</td>
                <td>{{address.zipcode}}</td>
                <td>{{website}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>